<?xml version="1.0"?><layout version="0.1.0">    <blog_adminhtml_blog_index>        <reference name="content">            <block type="blog/adminhtml_blog" name="blog" />        </reference>    </blog_adminhtml_blog_index>        <blog_adminhtml_blog_product>        <block type="core/text_list" name="root" output="toHtml">            <block type="blog/adminhtml_blog_edit_tab_product" name="catalog.product.edit.tab.blog"/>            <block type="adminhtml/widget_grid_serializer" name="product_grid_serializer">                <reference name="product_grid_serializer">                    <action method="initSerializerBlock">                        <grid_block_name>catalog.product.edit.tab.blog</grid_block_name>                        <data_callback>getSelectedBlogProducts</data_callback>                        <hidden_input_name>links[products]</hidden_input_name>                        <reload_param_name>blog_products</reload_param_name>                    </action>                    <action method="addColumnInputName">                        <input_name>position</input_name>                    </action>                </reference>            </block>        </block>    </blog_adminhtml_blog_product>        <blog_adminhtml_blog_productgrid>        <block type="core/text_list" name="root" output="toHtml">            <block type="blog/adminhtml_blog_edit_tab_product" name="catalog.product.edit.tab.blog"/>        </block>    </blog_adminhtml_blog_productgrid>		<blog_adminhtml_blog_categories>        <block type="core/text_list" name="root" output="toHtml">            <block type="blog/adminhtml_blog_edit_tab_categories" name="blog.edit.tab.categories"/>        </block>    </blog_adminhtml_blog_categories>	<blog_adminhtml_category_index>        <reference name="content">            <block type="blog/adminhtml_category" name="category" />        </reference>    </blog_adminhtml_category_index>		<!--faq_adminhtml_suggest_index>        <reference name="content">            <block type="faq/adminhtml_suggest" name="category" />        </reference>    </faq_adminhtml_suggest_index--></layout>