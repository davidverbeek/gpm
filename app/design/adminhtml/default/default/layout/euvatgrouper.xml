<?xml version="1.0"?>
<layout>
    <default>
        <reference name="notifications">
            <block type="euvatgrouper/adminhtml_notice" name="euvat_notice" template="euvatgrouper/notice.phtml" before="-" />
        </reference>
    </default>
    <adminhtml_sales_order_edit_index>
        <reference name="head">
            <action method="addJs">
                <script>geissweb/adminhtml/sales_order_edit.js</script>
            </action>
        </reference>
    </adminhtml_sales_order_edit_index>
    <euvatgrouper_adminhtml_setup_index>
        <reference name="content">
            <block type="euvatgrouper/adminhtml_setup" name="euvat_setup_index" />
        </reference>
    </euvatgrouper_adminhtml_setup_index>
    <euvatgrouper_adminhtml_setup_success>
        <reference name="content">
            <block type="adminhtml/template" name="euvat_setup_success" template="euvatgrouper/success.phtml"/>
        </reference>
    </euvatgrouper_adminhtml_setup_success>
</layout>