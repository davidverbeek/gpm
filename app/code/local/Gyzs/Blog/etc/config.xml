<?xml version="1.0"?><config>    <modules>        <Gyzs_Blog>            <version>1.0.0</version>        </Gyzs_Blog>    </modules>    <frontend>        <!--routers>            <faq>                <use>standard</use>                <args>                    <module>Magebuzz_Faq</module>                    <frontName>faq</frontName>                </args>            </faq>        </routers-->        <layout>            <updates>                <faq>                    <file>gyzsblog.xml</file>                </faq>            </updates>        </layout>            <translate>		<modules>                    <Gyzs_Blog>			<files>                            <default>Gyzs_Blog.csv</default>			</files>		    </Gyzs_Blog>		</modules>            </translate>    </frontend>    <admin>        <routers>            <blog>		<use>admin</use>                <args>                    <module>Gyzs_Blog</module>                    <frontName>blog</frontName>                </args>            </blog>        </routers>    </admin>    <adminhtml>	<layout>            <updates>		<blog>                    <file>blog.xml</file>		</blog>            </updates>	</layout>	<translate>            <modules>		<Gyzs_Blog>                    <files>			<default>Gyzs_Blog.csv</default>                    </files>		</Gyzs_Blog>            </modules>	</translate>    </adminhtml>       <global>	<models>            <blog>		<class>Gyzs_Blog_Model</class>		<resourceModel>blog_mysql4</resourceModel>            </blog>            <blog_mysql4>		<class>Gyzs_Blog_Model_Mysql4</class>                <entities>                    <blog>			<table>blog</table>                    </blog>                    <category>			<table>blog_category</table>                    </category>                    <blog_store>			<table>blog_store</table>                    </blog_store>		</entities>            </blog_mysql4>	</models>        <resources>            <blog_setup>		<setup>                    <module>Gyzs_Blog</module>		</setup>		<connection>                    <use>core_setup</use>		</connection>            </blog_setup>	</resources>	<blocks>            <blog>		<class>Gyzs_Blog_Block</class>            </blog>	</blocks>	<helpers>            <blog>		<class>Gyzs_Blog_Helper</class>            </blog>	</helpers>	<template>            <email>		<faq_suggest_email_template>                    <label>Suggestion from customer</label>                    <file>suggest_email.html</file>                    <type>html</type>		</faq_suggest_email_template>            </email>	</template>		    </global>	<default>	<faq>				<general>					<using_javascript>0</using_javascript>					<show_top_link>1</show_top_link>				</general>				<suggest>					<show_suggest_form>1</show_suggest_form>					<recipient_email>hello@example.com</recipient_email>					<sender_email_identity>general</sender_email_identity>					<email_template>faq_suggest_email_template</email_template>				</suggest>			</faq>		</default></config>