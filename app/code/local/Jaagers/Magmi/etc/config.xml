<?xml version="1.0"?>
<config>
  <modules>
    <Jaagers_Magmi>
      <version>0.1.1</version>
    </Jaagers_Magmi>
  </modules>
  <global>
    <helpers>
      <magmi>
        <class>Jaagers_Magmi_Helper</class>
      </magmi>
    </helpers>
	<models>
    <magmi>
        <class>Jaagers_Magmi_Model</class>
        <resourceModel>magmi_mysql4</resourceModel>
    </magmi>
	</models>
  <resources>
      <magmi_setup>
        <setup>
          <module>Jaagers_Magmi</module>
        </setup>
        <connection>
          <use>core_setup</use>
        </connection>
      </magmi_setup>
      <magmi_write>
        <connection>
          <use>core_write</use>
        </connection>
      </magmi_write>
      <magmi_read>
        <connection>
          <use>core_read</use>
        </connection>
      </magmi_read>
    </resources>
  </global>
