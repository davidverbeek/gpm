<?xml version="1.0"?>
<config>
    <modules>
        <Hs_SeoPagination>
            <version>1.0.3</version>
        </Hs_SeoPagination>
    </modules>
    <global>
    <events>
        <controller_action_layout_render_before_catalog_category_view>
            <observers>
                <seopagination>
                    <type>Singleton</type>
                    <class>Hs_SeoPagination_Model_Observer</class>
                    <method>doPagination</method>
                </seopagination>
            </observers>
        </controller_action_layout_render_before_catalog_category_view>
    </events>
    <models>
        <seopagination>
            <class>Hs_SeoPagination_Model</class>
        </seopagination>
    </models>
    <helpers>
        <seopagination>
            <class>Hs_SeoPagination_Helper</class>
        </seopagination>
    </helpers>
    </global>
</config>